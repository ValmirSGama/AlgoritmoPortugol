algoritmo "contadorDe1A10"
var
   C: Inteiro
inicio
      EscrevAL ("========================================================")
      EscrevaL ("ALGORÍTIMO PARA CONTAR DE 0 À 10 USANDO A ESTRUTURA PARA.")
      EscrevAL ("========================================================")
      Para C <- 10 Ate 1 Passo - 1 faca
           EscrevaL (C)
      FimPara
fimalgoritmo
